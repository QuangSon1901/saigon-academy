# Ignore directories generated by Drupal
/core
/vendor
/web/core

# Ignore Drupal configuration files
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore files generated by Drupal
/web/sites/*/files/
/web/sites/*/private/
/web/sites/*/temp/
/web/sites/*/backup_migrate/
/web/sites/*/simpletest/
/web/sites/*/cache/
/web/sites/*/logs/
/web/sites/*/phpstan.neon

# Ignore files generated by development tools
/.idea/
/.vscode/
/.phpcs-cache/
/.phpunit/
/.twig-cache/
/node_modules/
/yarn-error.log
/yarn.lock
/composer.lock

# Ignore files used in deployment
/deploy/
